package practice3;

import java.util.Scanner;

public class NumberMultiplication  // make a class name of NmberMultiplication
{
    public static void main(String[] args) // create main method
    {
        Scanner sc = new Scanner(System.in); // create object of scanner class
        int n = sc.nextInt(); // input the nymber from user
        for(int i =1; i<11;i++) // for loop
        {
            System.out.print(n*i+" "); // print all multiplication seprated by space
        }


    }
}

